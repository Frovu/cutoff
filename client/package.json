{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },    
    "rules": {
      "space-in-parens": [
        0,
        "always"
      ],
      "object-curly-spacing": [
        1,
        "always"
      ],
      "no-multiple-empty-lines": [
        1,
        {
          "max": 1,
          "maxEOF": 0,
          "maxBOF": 0
        }
      ],
      "quotes": [
        1,
        "single",
        "avoid-escape"
      ],
      "no-use-before-define": [
        2,
        {
          "functions": false
        }
      ],
      "semi": [
        1,
        "always"
      ],
      "prefer-const": 1,
      "react/prefer-es6-class": 0,
      "react/jsx-filename-extension": 0,
      "react/no-array-index-key": [
        1
      ],
      "no-undef": [
        2
      ],
      "no-case-declarations": [
        1
      ],
      "no-return-assign": [
        1
      ],
      "no-param-reassign": [
        1
      ],
      "no-shadow": [
        1
      ],
      "camelcase": [
        1
      ],
      "no-underscore-dangle": [
        0,
        "always"
      ],
      "indent": [
        1,
        "tab"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
